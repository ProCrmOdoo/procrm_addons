<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record model="ir.ui.view" id="view_crm_project_project_form">
            <field name="name">view_project_project_form</field>
            <field name="model">project.project</field>
            <field name="inherit_id" ref="project.edit_project"/>
            <field name="arch" type="xml">
                <xpath expr="//sheet/notebook/field[@name='partner_id']" position="after">
                    <field name="sales_manager_id"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>

